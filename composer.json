{
    "name": "mdixon18/media-library",
    "description": "Media Library with VueJS",
    "keywords": [
        "media", "library"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Dixon",
            "email": "mdixon14717@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "mdixon18/laravel-medialibrary": "^7.6"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/database": "3.7.*",
        "orchestra/testbench": "3.7.*",
        "phpunit/phpunit": "~7.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Mdixon18\\MediaLibrary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mdixon18\\MediaLibrary\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Mdixon18\\MediaLibrary\\MediaLibraryServiceProvider"
            ],
            "aliases": {
                "MediaLibrary": "Mdixon18\\MediaLibrary\\MediaLibrary"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
